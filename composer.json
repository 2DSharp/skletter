{
  "name": "twodee/skletter",
  "type": "application",
  "homepage": "https://github.com/2dsharp/skletter",
  "description": "Social diary",

  "authors": [
    {
      "name": "Dedipyaman Das",
      "email": "2d@twodee.me",
      "role": "Developer"
    }
  ],

  "autoload": {
    "psr-4": {
      "Skletter\\": "src/"
    },
    "files": [
      "src/Factory/DependencyFactory.php"
    ]
  },

  "autoload-dev": {
    "psr-4": {
      "Skletter\\UnitTest\\" : "tests/UnitTests/"
    }
  },
  "repositories": [ {
    "type": "vcs",
    "url": "https://github.com/2dsharp/greentea.git"
  }],
  "require": {
    "php": ">=7.4",
    "ext-pdo": "*",
    "ext-openssl": "*",
    "psr/log": "1.*",
    "ext-mbstring": "*",
    "twig/twig": "^2.0",
    "symfony/http-foundation": "^4.2",
    "nikic/fast-route": "^1.2",
    "rdlowrey/auryn": "^1.4",
    "symfony/config": "^3.2",
    "monolog/monolog": "^1.24",
    "vlucas/phpdotenv": "^3.4",
    "ocramius/proxy-manager": "^2.2",
    "ext-json": "*",
    "predis/predis": "^1.1",
    "php-amqplib/php-amqplib": "^2.10",
    "apache/thrift": "^0.12.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^7",
    "mockery/mockery": "^1.1",
    "squizlabs/php_codesniffer": "3.*"
  },

  "config": {
    "platform": {
      "php": "7.4.0RC4"
    },
    "platform-dev": {
      "php": "7.4.0RC4"
    }
  }
}
