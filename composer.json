{
  "name": "twodee/skletter",
  "type": "application",
  "homepage": "https://github.com/2dsharp/skletter",
  "description": "Social diary",

  "authors": [
    {
      "name": "Dedipyaman Das",
      "email": "2d@twodee.me",
      "role": "Developer"
    }
  ],

  "autoload": {
    "psr-4": { "Skletter\\": "src/" }
  },

  "autoload-dev": {
    "psr-4": {
      "Skletter\\UnitTest\\" : "tests/UnitTests/"
    }
  },
  "repositories": [ {
    "type": "package",
    "package": {
      "name": "twodee/greentea",
      "version": "dev-master",
      "source": {
        "url": "https://github.com/2dsharp/greentea.git",
        "type": "git",
        "reference": "master"
      }
    }
  }],
  "require": {
    "php": ">=7.2",
    "ext-pdo": "*",
    "psr/log": "1.*",
    "ext-mbstring": "*",
    "twodee/phypes": "dev-master",
    "twodee/greentea": "dev-master",
    "twig/twig": "^2.0",
    "symfony/http-foundation": "^3.3",
    "nikic/fast-route": "^1.2",
    "rdlowrey/auryn": "^1.4",
    "symfony/config": "^3.2",
    "monolog/monolog": "^1.24",
    "vlucas/phpdotenv": "^3.4"

  },
  "require-dev": {
    "phpunit/phpunit": "^7",
    "mockery/mockery": "^1.1"
  },

  "config": {
    "platform": {
      "php": "7.2.9"
    },
    "platform-dev": {
      "php": "7.2.9"
    }
  }
}
