{
  "name": "twodee/skletter",
  "type": "application",
  "homepage": "https://github.com/2dsharp/skletter",
  "description": "Social diary",

  "authors": [
    {
      "name": "Dedipyaman Das",
      "email": "2d@twodee.me",
      "role": "Developer"
    }
  ],

  "autoload": {
    "psr-4": { "Skletter\\": "src/" }
  },

  "autoload-dev": {
    "psr-4": {
      "Skletter\\UnitTest\\" : "tests/UnitTests/"
    }
  },
  "repositories": [ {
    "type": "vcs",
    "url": "https://github.com/2dsharp/greentea.git"
  }],
  "require": {
    "php": ">=7.2",
    "ext-pdo": "*",
    "psr/log": "1.*",
    "ext-mbstring": "*",
    "twodee/phypes": "dev-master",
    "twodee/greentea": "dev-master",
    "twig/twig": "^2.0",
    "symfony/http-foundation": "^4.2",
    "nikic/fast-route": "^1.2",
    "rdlowrey/auryn": "^1.4",
    "symfony/config": "^3.2",
    "monolog/monolog": "^1.24",
    "vlucas/phpdotenv": "^3.4",
    "ocramius/proxy-manager": "^2.2"

  },
  "require-dev": {
    "phpunit/phpunit": "^7",
    "mockery/mockery": "^1.1"
  },

  "config": {
    "platform": {
      "php": "7.2.9"
    },
    "platform-dev": {
      "php": "7.2.9"
    }
  }
}
